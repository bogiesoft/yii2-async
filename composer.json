{
    "name": "bazilio/yii2-async",
    "description": "Provides translucent api for moving large tasks out of request context",
    "license": "MIT",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Vasily Ostanin",
            "email": "bazilio91@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "keywords": ["yii2", "async", "amqp", "rabbitmq", "tasks", "redis"],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "yiisoft/yii2-codeception": "*",
        "codeception/codeception": "2.0.*",
        "pdezwart/php-amqp": "dev-master",
        "E96/yii2-redis": "*",
        "kriswallsmith/spork": "*"
    },
    "autoload": {
        "psr-4": {
            "bazilio\\async\\": ""
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "pdezwart/php-amqp",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/pdezwart/php-amqp",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:E96/yii2-redis.git"
        }
    ]
}
